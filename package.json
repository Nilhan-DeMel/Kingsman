{
  "name": "kingsman",
  "displayName": "Kingsman",
  "description": "Skill Store for Antigravity agents - discover, inspect, and install skills from GitHub",
  "version": "0.2.1",
  "publisher": "nilhan-demel",
  "license": "MIT",
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nilhan-DeMel/Kingsman.git"
  },
  "bugs": {
    "url": "https://github.com/Nilhan-DeMel/Kingsman/issues"
  },
  "homepage": "https://github.com/Nilhan-DeMel/Kingsman#readme",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "skills",
    "agent",
    "store",
    "marketplace",
    "github",
    "mcp",
    "google",
    "search"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kingsman.store.open",
        "title": "Kingsman: Skill Store"
      },
      {
        "command": "kingsman.googleSearch",
        "title": "Kingsman: Google Search"
      },
      {
        "command": "kingsman.googleSearchUrl",
        "title": "Kingsman: Get Search URL (Agent)"
      },
      {
        "command": "kingsman.googleSearchWriteArtifact",
        "title": "Kingsman: Search and Write Artifact (Agent)"
      },
      {
        "command": "kingsman.googleSearchOpenAndWrite",
        "title": "Kingsman: Search, Open, and Write Artifact (Agent)"
      },
      {
        "command": "kingsman.store.search",
        "title": "Kingsman: Store Search (Agent)"
      },
      {
        "command": "kingsman.store.inspect",
        "title": "Kingsman: Store Inspect (Agent)"
      },
      {
        "command": "kingsman.store.stage",
        "title": "Kingsman: Store Stage (Agent)"
      },
      {
        "command": "kingsman.store.install",
        "title": "Kingsman: Store Install (Agent)"
      },
      {
        "command": "kingsman.store.listInstalled",
        "title": "Kingsman: Store List Installed (Agent)"
      },
      {
        "command": "kingsman.store.uninstall",
        "title": "Kingsman: Store Uninstall (Agent)"
      }
    ],
    "configuration": {
      "title": "Kingsman",
      "properties": {
        "kingsman.searchBaseUrl": {
          "type": "string",
          "default": "https://www.google.com/search?q=",
          "description": "Base URL for Google searches"
        },
        "kingsman.globalSkillsPath": {
          "type": "string",
          "default": "",
          "description": "Custom path for global skills folder (leave empty for default)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.10.0",
    "@vscode/vsce": "^2.22.0",
    "typescript": "^5.3.0"
  }
}